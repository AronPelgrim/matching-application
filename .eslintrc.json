{
    "extends": [
      "wesbos"
    ],
    "eslint": {
      "@disable": false,
      "args": [],
      "excludes": ["*/*.html"]
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es6",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 8
        }
      ]
    }
  }